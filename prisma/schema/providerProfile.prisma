

model ProviderProfile{
    id String @id @default(uuid())
    resturantName String
    userInfo User @relation(fields: [userId],references: [id])
    userId String @unique
    phone String 
    address String
    averageRating Float @default(0)
    totalreviews Int @default(0)

    meals Meal[]

    orders Order[]

    reviews Review[]
}